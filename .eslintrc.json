{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quotes": ["error", "double"],
    "max-len": ["error", {"code": 120, "ignoreUrls": true }],
    "comma-dangle": ["error", "never"],
    "strict": "error"
  }
}
